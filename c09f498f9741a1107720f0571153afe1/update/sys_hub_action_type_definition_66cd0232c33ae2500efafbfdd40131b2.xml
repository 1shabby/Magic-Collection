        <value>(function execute(inputs, outputs) {&#13;
    var card = inputs.card;&#13;
    var response = JSON.parse(inputs.response);&#13;
&#13;
    // Update basic fields if present&#13;
    var topFields = [&#13;
        'rarity',&#13;
        'mana_cost', 'type_line', 'oracle_text', 'power',&#13;
        'toughness', 'loyalty', 'lang'&#13;
    ];&#13;
&#13;
    topFields.forEach(function(field) {&#13;
        if (response[field] !== undefined &amp;&amp; response[field] !== null) {&#13;
            card.setValue(field, response[field]);&#13;
        }&#13;
    });&#13;
&#13;
    // Handle pricing fields - only set if existing value is 0 or null&#13;
    var priceFields = ['usd', 'usd_foil', 'usd_etched'];&#13;
    priceFields.forEach(function(priceKey) {&#13;
        var newVal = response.prices ? response.prices[priceKey] : null;&#13;
        var currentVal = parseFloat(card.getValue(priceKey)) || 0;&#13;
&#13;
        if ((currentVal === 0 || isNaN(currentVal)) &amp;&amp; newVal) {&#13;
            card.setValue(priceKey, newVal);&#13;
        }&#13;
    });&#13;
&#13;
    // Handle double-faced cards&#13;
    if (Array.isArray(response.card_faces)) {&#13;
        var front = response.card_faces[0];&#13;
        var back = response.card_faces[1];&#13;
&#13;
        if (front) {&#13;
            if (front.oracle_text) card.setValue('front_oracle_text', front.oracle_text);&#13;
            if (front.name) card.setValue('front_name', front.name);&#13;
            if (front.type_line) card.setValue('front_type_line', front.type_line);&#13;
            if (front.mana_cost) card.setValue('front_mana_cost', front.mana_cost);&#13;
        }&#13;
&#13;
        if (back) {&#13;
            if (back.oracle_text) card.setValue('back_oracle_text', back.oracle_text);&#13;
            if (back.name) card.setValue('back_name', back.name);&#13;
            if (back.type_line) card.setValue('back_type_line', back.type_line);&#13;
            if (back.mana_cost) card.setValue('back_mana_cost', back.mana_cost);&#13;
        }&#13;
    }&#13;
&#13;
    // If single-faced, and top-level oracle_text was missing, fallback to face[0]&#13;
    else if (!response.oracle_text &amp;&amp; response.card_faces &amp;&amp; response.card_faces[0]) {&#13;
        var face = response.card_faces[0];&#13;
        if (face.oracle_text) card.setValue('oracle_text', face.oracle_text);&#13;
    }&#13;
&#13;
    // Mark for update in flow — don’t call update() in action script steps&#13;
    outputs.card = card;&#13;
&#13;
})(inputs, outputs);</value>
