    <sys_script_fix action="DELETE">
        <before>false</before>
        <description>Updates Cards for Greg</description>
        <name>MTG Update Owner Reference</name>
        <record_for_rollback>true</record_for_rollback>
        <script><![CDATA[var cards = new GlideRecord('x_759224_magic_c_0_mtg_cards');
cards.addEncodedQuery('nameISNOTEMPTY');
gs.info(gs.getUserID());
cards.query();
while(cards.next()){
	cards.setValue('owner',gs.getUserID());
	cards.update();
}	]]></script>
